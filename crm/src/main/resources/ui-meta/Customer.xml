<?xml version="1.0" encoding="utf-8"?>
<root namespace="com.huoyun.sbo" name="Customer">
	<header>
		<title ref="name"></title>
		<subtitle ref=""></subtitle>
	</header>
	<sections>
		<section label="uimeta.Customer.section.customerInfo">
			<properties>
				<property ref="name"></property>
				<property ref="telephone"></property>
				<property ref="salesSource"></property>
				<property ref="salesman"></property>
				<property ref="finishType"></property>
			</properties>
		</section>
		<section label="uimeta.Customer.section.houseInfo">
			<properties>
				<property ref="houseArea"></property>
				<property ref="houseType"></property>
				<property ref="completionDate"></property>
				<property ref="houses"></property>
				<property ref="address"></property>
			</properties>
		</section>
		<section label="uimeta.Customer.section.designInfo">
			<properties>
				<property ref="status"></property>
				<property ref="measureDate"></property>
				<property ref="designer"></property>
				<property ref="cadDrawer"></property>
				<property ref="effectDrawer"></property>
			</properties>
		</section>
		<section label="uimeta.Customer.section.constructionInfo">
			<properties>
				<property ref="constructionStatus"></property>
				<property ref="projectManager"></property>
				<property ref="constructionLeader"></property>
				<property ref="constructionStartDate"></property>
				<property ref="constructionEndDate"></property>
			</properties>
		</section>
	</sections>
	<listview select="id,name,contact" orderby="createTime desc">
		<columns>
			<column ref="name"></column>
			<column ref="telephone"></column>
			<column ref="houseArea"></column>
			<column ref="houseType"></column>
			<column ref="salesSource"></column>
			<column ref="status"></column>
		</columns>
	</listview>
	<filters>
		<filter label="ALL" expression=""></filter>
	</filters>
	<properties>
		<property name="id"></property>
		<property name="name"></property>
		<property name="telephone"></property>
		<property name="houses"></property>
		<property name="address"></property>
		<property name="salesman"></property>
		<property name="designer"></property>
		<property name="houseArea"></property>
		<property name="houseType"></property>
		<property name="completionDate"></property>
		<property name="finishType"></property>
		<property name="cadDrawer"></property>
		<property name="effectDrawer"></property>
		<property name="salesSource"></property>
		<property name="status"></property>
		<property name="constructionStatus"></property>
		<property name="projectManager"></property>
		<property name="constructionLeader"></property>
		<property name="constructionStartDate"></property>
		<property name="constructionEndDate"></property>
		<property name="measureDate"></property>
	</properties>
</root>